.slide_up_text
	*
		overflow hidden
		display block
		transform: translate(0, 150%)
		transition-delay: 0s;
		transition $slide_up_text
		
	&.slide_up_text--active
		*
			display block
			transition $slide_up_text
			transform: translate(0, 0%)
		li,div
			&:nth-child(1)
				> a, > span
					transition-delay: 0s
		li,div
			&:nth-child(2)
				> a, > span
					transition-delay: 0.2s
		li,div
			&:nth-child(3)
				> a, > span
					transition-delay: 0.4s
		li,div
			&:nth-child(4)
				> a, > span
					transition-delay: 0.6s
		li,div
			&:nth-child(5)
				> a, > span
					transition-delay: 0.8s
		li,div
			&:nth-child(6)
				> a, > span
					transition-delay: 0.10s
		li,div
			&:nth-child(7)
				> a, > span
					transition-delay: 0.12s
		li,div
			&:nth-child(8)
				> a, > span
					transition-delay: 0.14s
		li,div
			&:nth-child(9)
				> a, > span
					transition-delay: 0.26s